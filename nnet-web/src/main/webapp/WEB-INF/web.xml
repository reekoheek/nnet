<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>InitServlet</servlet-name>
		<servlet-class>
			com.xinixtech.nnet.web.servlet.InitServlet
		</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet>
		<servlet-name>NeuralServlet</servlet-name>
		<display-name>NeuralServlet</display-name>
		<description></description>
		<servlet-class>
			com.xinixtech.nnet.web.servlet.NeuralServlet
		</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>NeuralServlet</servlet-name>
		<url-pattern>/NeuralServlet</url-pattern>
	</servlet-mapping>
</web-app>
